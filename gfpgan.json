{
    "engine": "gfpgan",
    "name": "Face Restauration & Hi-Res",
    "location": "",
    "ip": "0.0.0.0",
    "port": 5012,
    "version": "1.0.6",
    "description": "Image Utility",
    "github": "https://github.com/TencentARC/GFPGAN",
    "requiresGPU": true,
    "isFree": false,
    "default_cost": 1000,
    "params": [{
        "in": "url_upload",
        "out": "-filename",
        "isMandatory": true,
        "type": "string",
        "default": null,
        "accept": {
            "$max": 2048,
            "$ext": [".png", ".jpg", ".jpeg"]
        },
        "ui": {
            "title": "Image input",
            "widget": "uiSelectPicture",
            "column": 1
        }
    }, {
        "in": "res",
        "out": "-s",
        "isMandatory": true,
        "type": "int",
        "default": 1,
        "accept": {
            "$in":[1, 2, 4], 
            "display":["x1", "x2", "x4"]
        },
        "freeAccess": {"$max": 1},
        "ui": {
            "title": "Output resolution",
            "widget": "uiMultiToggle",
            "unit": "x",            
            "column": 2
        }
    }, {
        "in": "weight",
        "out": "-w",
        "isMandatory": false,
        "type": "float",
        "default": 0.5
    }, {
        "in": "ver",
        "out": "-v",
        "isMandatory": false,
        "type": "string",
        "default": "1.3"
    }, {
        "in": "output",
        "out": "-o",
        "isMandatory": true,
        "type": "function",
        "default": "$output_uid.ext"
    }, {
        "in": "idir",
        "out": "-idir",
        "isMandatory": false,
        "type": "string",
        "default": "./_input/"
    }, {
        "in": "odir",
        "out": "-odir",
        "isMandatory": false,
        "type": "string",
        "default": "./_output/"
    }]
}
